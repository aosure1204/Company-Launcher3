                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableLeft="@drawable/ic_music_title"
                    android:text="@string/music"
                    android:textColor="#FFFFFF"
                    android:textSize="@dimen/app_title_text_size"
                    android:layout_gravity="center_horizontal"
                    android:gravity="center"
                    android:padding="5dp" />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_music" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/app_mobile_internet"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/app_margin_left"
                android:layout_marginTop="@dimen/app_margin_top"
                android:layout_marginRight="@dimen/app_margin_right"
                android:layout_marginBottom="@dimen/app_margin_bottom"
                android:background="@drawable/bg_app"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:clickable="true">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableLeft="@drawable/ic_mobile_internet_title"
                    android:text="@string/mobile_internet"
                    android:textColor="#FFFFFF"
                    android:textSize="@dimen/app_title_text_size"
                    android:layout_gravity="center_horizontal"
                    android:gravity="center"
                    android:padding="5dp" />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_mobile_internet"/>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:gravity="center_horizontal"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/app_air_conditioner"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/app_margin_left"
                android:layout_marginTop="@dimen/app_margin_top"
                android:layout_marginRight="@dimen/app_margin_right"
                android:layout_marginBottom="@dimen/app_margin_bottom"
                android:background="@drawable/bg_app"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:clickable="true">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableLeft="@drawable/ic_air_title"
                    android:text="@string/air"
                    android:textColor="#FFFFFF"
                    android:textSize="@dimen/app_title_text_size"
                    android:layout_gravity="center_horizontal"
                    android:gravity="center"
                    android:padding="5dp" />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_air" />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/app_navigation"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/app_margin_left"
                android:layout_marginTop="@dimen/app_margin_top"
                android:layout_marginRight="@dimen/app_margin_right"
                android:layout_marginBottom="@dimen/app_margin_bottom"
                android:background="@drawable/bg_app"
                android:gravity="center_horizontal"
