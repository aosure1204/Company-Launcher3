                android:clickable="true">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableLeft="@drawable/ic_navigation_map_title"
                    android:text="@string/navigation_map"
                    android:textColor="#FFFFFF"
                    android:textSize="@dimen/app_title_text_size"
                    android:layout_gravity="center_horizontal"
                    android:gravity="center"
                    android:padding="5dp" />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_navigation_map" />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/app_radio"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/app_margin_left"
                android:layout_marginTop="@dimen/app_margin_top"
                android:layout_marginRight="@dimen/app_margin_right"
