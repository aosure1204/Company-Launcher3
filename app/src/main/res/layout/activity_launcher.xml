<?xml version="1.0" encoding="utf-8"?>
<com.wd.launcher3.Workspace xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_quanju" >

    <com.wd.launcher3.CellLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_horizontal"
        android:padding="40dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/app_settings"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/app_margin_left"
                android:layout_marginTop="@dimen/app_margin_top"
                android:layout_marginRight="@dimen/app_margin_right"
                android:layout_marginBottom="@dimen/app_margin_bottom"
                android:background="@drawable/bg_app"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:clickable="true">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableLeft="@drawable/ic_system_settings_title"
                    android:text="@string/system_settings"
                    android:textColor="#FFFFFF"
                    android:textSize="@dimen/app_title_text_size"
                    android:layout_gravity="center_horizontal"
                    android:gravity="center"
                    android:padding="5dp" />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_system_settings" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/folder_online_apps"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/app_margin_left"
                android:layout_marginTop="@dimen/app_margin_top"
                android:layout_marginRight="@dimen/app_margin_right"
                android:layout_marginBottom="@dimen/app_margin_bottom"
                android:background="@drawable/bg_app"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:clickable="true">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableLeft="@drawable/ic_online_apps_title"
                    android:text="@string/online_apps"
                    android:textColor="#FFFFFF"
                    android:textSize="@dimen/app_title_text_size"
                    android:layout_gravity="center_horizontal"
                    android:gravity="center"
                    android:padding="5dp" />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_online_apps" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/app_vdr"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/app_margin_left"
                android:layout_marginTop="@dimen/app_margin_top"
                android:layout_marginRight="@dimen/app_margin_right"
                android:layout_marginBottom="@dimen/app_margin_bottom"
                android:background="@drawable/bg_app"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:clickable="true">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableLeft="@drawable/ic_driving_data_title"
                    android:text="@string/driving_data"
                    android:textColor="#FFFFFF"
                    android:textSize="@dimen/app_title_text_size"
                    android:layout_gravity="center_horizontal"
                    android:gravity="center"
                    android:padding="5dp" />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_driving_data" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/app_ebook"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/app_margin_left"
                android:layout_marginTop="@dimen/app_margin_top"
                android:layout_marginRight="@dimen/app_margin_right"
                android:layout_marginBottom="@dimen/app_margin_bottom"
                android:background="@drawable/bg_app"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:clickable="true">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableLeft="@drawable/ic_e_book_title"
                    android:text="@string/e_book"
                    android:textColor="#FFFFFF"
                    android:textSize="@dimen/app_title_text_size"
                    android:layout_gravity="center_horizontal"
                    android:gravity="center"
                    android:padding="5dp" />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_e_book" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:gravity="center_horizontal"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/folder_common_tool"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/app_margin_left"
                android:layout_marginTop="@dimen/app_margin_top"
                android:layout_marginRight="@dimen/app_margin_right"
                android:layout_marginBottom="@dimen/app_margin_bottom"
                android:background="@drawable/bg_app"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:clickable="true">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableLeft="@drawable/ic_common_tool_title"
                    android:text="@string/common_tool"
                    android:textColor="#FFFFFF"
                    android:textSize="@dimen/app_title_text_size"
                    android:layout_gravity="center_horizontal"
                    android:gravity="center"
                    android:padding="5dp" />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_common_tool" />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/app_drive_assist"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/app_margin_left"
                android:layout_marginTop="@dimen/app_margin_top"
                android:layout_marginRight="@dimen/app_margin_right"
                android:layout_marginBottom="@dimen/app_margin_bottom"
                android:background="@drawable/bg_app"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:clickable="true">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableLeft="@drawable/ic_traffic_assistance_title"
                    android:text="@string/traffic_assistance"
                    android:textColor="#FFFFFF"
                    android:textSize="@dimen/app_title_text_size"
                    android:layout_gravity="center_horizontal"
                    android:gravity="center"
                    android:padding="5dp" />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_traffic_assistance" />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/app_weather"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/app_margin_left"
                android:layout_marginTop="@dimen/app_margin_top"
                android:layout_marginRight="@dimen/app_margin_right"
                android:layout_marginBottom="@dimen/app_margin_bottom"
                android:background="@drawable/bg_app"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:clickable="true">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableLeft="@drawable/ic_weather_title"
                    android:text="@string/weather"
                    android:textColor="#FFFFFF"
                    android:textSize="@dimen/app_title_text_size"
                    android:layout_gravity="center_horizontal"
                    android:gravity="center"
                    android:padding="5dp" />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_weather" />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/app_photo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/app_margin_left"
                android:layout_marginTop="@dimen/app_margin_top"
                android:layout_marginRight="@dimen/app_margin_right"
                android:layout_marginBottom="@dimen/app_margin_bottom"
                android:background="@drawable/bg_app"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:clickable="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="5dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_photos_title" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/photos"
                        android:textColor="#FFFFFF"
                        android:textSize="@dimen/app_title_text_size" />
                </LinearLayout>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_photos" />
            </LinearLayout>
        </LinearLayout>
    </com.wd.launcher3.CellLayout>

    <com.wd.launcher3.CellLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_horizontal"
        android:padding="40dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/app_car_setting"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/app_margin_left"
                android:layout_marginTop="@dimen/app_margin_top"
                android:layout_marginRight="@dimen/app_margin_right"
                android:layout_marginBottom="@dimen/app_margin_bottom"
                android:background="@drawable/bg_app"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:clickable="true">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableLeft="@drawable/ic_vehicle_control_title"
                    android:text="@string/vehicle_control"
